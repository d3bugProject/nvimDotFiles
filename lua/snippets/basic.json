{
  "simple view": {
    "prefix": "vw",
    "body": ["<View> $1 </View>"],
    "description": "simple view",
    "import": "import {View} from 'react-native'"
  },
  "router with import ": {
    "prefix": "router",
    "body": ["const router = useRouter()"],
    "description": "router with import "
  },
  "arrow variable": {
    "prefix": "av",
    "body": ["const $1 = ()=>{", "  $2", "}"],
    "description": "arrow variable"
  },
  "arrow variable": {
    "prefix": "a",
    "body": ["()=>{", "  $2", "}"],
    "description": "arrow variable"
  },
  "function": {
    "prefix": "func",
    "body": ["function $1 (){  ", "  return $2", "}"],
    "description": "function"
  },
  "export function or component": {
    "prefix": "exfu",
    "body": [
      "export default function ${TM_FILENAME_BASE/(.)(.*)/${1:/upcase}${2}/}(){",
      "  return $2",
      "}"
    ],
    "description": "export function or component"
  }
}
