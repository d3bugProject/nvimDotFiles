{
  "header_comment": {
    "prefix": "header",
    "body": [
      "/**",
      " * @file ${TM_FILENAME}",
      " * @author ${1}",
      " * @date ${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR} : ${CURRENT_HOUR}",
      " * @description ${2}",
      " */"
    ]
  },

  "arrow variable": {
    "prefix": "av",
    "body": ["const ${1} = ()=>{", "  ${2}", "}"],
    "description": "arrow variable"
  },
  "arrow variable": {
    "prefix": "a",
    "body": ["(${3})=>{", "  ${1} ${0} ", "}"],
    "description": "arrow variable"
  },
  "function": {
    "prefix": "func",
    "body": ["function $1 (){  ", "  return $2", "}"],
    "description": "function"
  },
  "export function or component": {
    "prefix": "exfu",
    "body": [
      "export default function ${TM_FILENAME_PASCAL}(){",
      "  return $2",
      "}"
    ],
    "description": "export function or component"
  },
  "function": {
    "prefix": "xxxx",
    "body": ["$1    $2     $3    "],
    "description": "function"
  },
  "classname": {
    "prefix": "cn",
    "body": ["className={` $1 `}"],
    "description": "classname"
  }
}
